    "FSharp.dotnetRoot": "/usr/share/dotnet",
    "FSharp.workspacePath": "./src/Contextive.sln",
    "vsCodeExtensionPath": "./src/vscode/contextive",
    "languageServerPath": "./src/Contextive.LanguageServer",
    "languageServerTestsPath": "./src/Contextive.LanguageServer.Tests",
    "files.exclude": {
        "out": false, // set this to true to hide the "out" folder with the compiled JS files
        "dist": false // set this to true to hide the "dist" folder with the compiled JS files
    },
    "search.exclude": {
        "out": true, // set this to false to include "out" folder in search results
        "dist": true // set this to false to include "dist" folder in search results
    },
    "dotnet-test-explorer.autoWatch": true,
    "dotnet-test-explorer.testProjectPath": "**/*.Tests.fsproj",
    "FSharp.enableAdaptiveLspServer": true
}